{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "Chart.js@4.2.1",
      "destination": "wwwroot/lib/Chart.js/"
    },
    {
      "library": "fontawesome-iconpicker@3.2.0",
      "destination": "wwwroot/lib/fontawesome-iconpicker/"
    },
    {
      "library": "font-awesome@6.3.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "sweetalert2@11.7.3",
      "destination": "wwwroot/lib/sweetalert2/"
    },
    {
      "provider": "unpkg",
      "library": "@ckeditor/ckeditor5-autoformat@34.1.0",
      "destination": "wwwroot/lib/ckeditor/ckeditor5-autoformat/"
    },
    {
      "provider": "unpkg",
      "library": "@ckeditor/ckeditor5-basic-styles@34.1.0",
      "destination": "wwwroot/lib/ckeditor/ckeditor5-basic-styles/"
    }
  ]
}